<div class="container">
<div class="row pb-4">
<% if current_user %>
<div class="col-md-6"></div>
<div class="col-md-6">
    Logged in as <%= current_user.email %>
    <%= link_to "My account", posts_path, class: "btn btn-primary" %>
    <%= link_to "Log Out", logout_path, class: "btn btn-primary"  %>
    </div>
<% else %>
<div class="col-md-8"></div>
<div class="col-md-4 ">
  <%= link_to "Sign Up", signup_path, class: "btn btn-primary" %>  
  <%= link_to "Log In", login_path, class: "btn btn-primary" %>
  </div>
<% end %>
</div>
    <h1 class="pb-2">Avialable Users</h1>

      <% @users.each do |user| %>
      <div class="pb-4">
        <div><%= user.email %></div>
        <%= link_to "User Posts", user_posts_path(user_id: user.id), class: "btn btn-dark" %>
        </div>
    <% end %>
</div>
</div>